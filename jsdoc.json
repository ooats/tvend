{
    "plugins": ["plugins/markdown"],
    "source": {
        "include": ["src"],
        "includePattern": ".ts$",
        "excludePattern": "(node_modules/|docs)"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
        "opts": {
            "destination": "./jsdoc",         
            "recurse": true                
        }
    
}
